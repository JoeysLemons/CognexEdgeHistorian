<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <add key="Location" value="AdamsTesting" />
    <add key="AcquisitionCountNodeID" value="ns=2;s=Tasks.InspectionTask.Spreadsheet.AcquisitionCount" />
  </appSettings>
  <CognexCameras>
    <!--Add Cameras here as needed follow camera template-->
    <Camera name="CameraTemplate" endpoint="ipAddress:port">
      <MonitoredTags>
        <!--Add tags here as needed follow tag template updateRate is in milliseconds-->
        <Tag browseName="browseName" name="templateTag" updateRate="1000" nodeID="nodeId"/>
      </MonitoredTags>
    </Camera>
  </CognexCameras>
  <connectionStrings>
    <add name="MainConnectionString" connectionString="Data Source=(localdb)\EdgeHistorian;Initial Catalog=EdgeHistorian;Integrated Security=True"
      providerName="System.Data.SqlClient" />
  </connectionStrings>
</configuration>